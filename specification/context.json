    "map": "avro:map",
    "type": {
        "@id": "avro:type",
        "@type": "@vocab"
    },
    "fields": {
        "@id": "avro:fields",
        "@container": "@list"
    },
    "symbols": {
        "@id": "avro:symbols",
        "@container": "@list"
    },
    "items": {
        "@id": "avro:items",
        "@type": "@vocab"
    },
    "values": {
        "@id": "avro:values",
        "@type": "@vocab"
    },

    "inputs": "wfdesc:hasInput",
    "outputs": "wfdesc:hasOutput",

    "CommandLineTool": "cwl:CommandLineTool",
    "Expression": "cwl:Expression",
    "JavascriptExpression": "cwl:JavascriptExpression",

    "requirements": "cwl:requirement",

    "CPURequirement": "cwl:CPURequirement",
    "MemoryRequirement": "cwl:MemoryRequirement",

    "DockerImage": "cwl:DockerImage",
