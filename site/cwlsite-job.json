#!/usr/bin/env cwl-runner
{
    "cwl:tool": "cwlsite.cwl",

    "schema_in": {
        "class": "File",
        "path": "../draft-3/Workflow.yml"
    },
    "context_target": "draft-3/cwl-context.json",
    "rdfs_target": "draft-3/cwl-rdfs.ttl",

    "render": [
        {
            "source": {
                "class": "File",
                "path": "../README.md"
            },
            "target": "index.html",
            "renderlist": []
        },
        {
            "source": {
                "class": "File",
                "path": "../draft-3/README.md"
            },
            "target": "draft-3/index.html",
            "renderlist": []
        },
        {
            "source": {
                "class": "File",
                "path": "../draft-3/Workflow.yml"
            },
            "target": "draft-3/CommandLineTool.html",
            "renderlist": [
                "https://w3id.org/cwl/cwl#CommandLineToolDoc"
            ]
        },
        {
            "source": {
                "class": "File",
                "path": "../draft-3/Workflow.yml"
            },
            "target": "draft-3/Workflow.html",
            "renderlist": [
                "https://w3id.org/cwl/cwl#WorkflowDoc"
            ]
        },
        {
            "source": {
                "class": "File",
                "path": "../draft-3/salad/schema_salad/metaschema.yml"
            },
            "target": "draft-3/SchemaSalad.html",
            "renderlist": []
        }
    ]
}
